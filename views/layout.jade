doctype html
html
  head
    title= title
    link(rel='stylesheet', href='/stylesheets/style.css')
    script(type='text/javascript' src='javascripts/jquery-1.11.3.min.js')
    script(type='text/javascript' src='javascripts/app.js')
  body
    form(
      method='post'
      )
      label first name
      input(
        type='text'
        id='first'
        )
      label last name
      input(
        type='text'
        id='last'
        )
      input(
        type='submit'
        value='submit'
        class='submit-button'
      )
  block content

